{
  "$schema": "https://developer.microsoft.com/json-schemas/api-extractor/v7/api-extractor.schema.json",
  "mainEntryPointFilePath": "<projectFolder>/api-extractor/types/index.d.ts",
  "bundledPackages": ["project_a"],

  "apiReport": {
    "enabled": true,
    "reportFolder": "<projectFolder>/api-extractor/",
    "reportTempFolder": "<projectFolder>/temp/"
  },

  "docModel": {
    "enabled": false
  },

  "dtsRollup": {
    "enabled": true,
    "untrimmedFilePath": "",
    "publicTrimmedFilePath": "<projectFolder>/index.d.ts",
    "omitTrimmingComments": true
  },

  "tsdocMetadata": {
    "enabled": false,
    "tsdocMetadataFilePath": "<projectFolder>/release/tsdoc-metadata.json"
  },

  "messages": {
    "compilerMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    },

    "extractorMessageReporting": {
      "default": {
        "logLevel": "warning"
      },
      "ae-forgotten-export": {
        "logLevel": "warning",
        "addToApiReportFile": true
      }
    },

    "tsdocMessageReporting": {
      "default": {
        "logLevel": "warning"
      }
    }
  }
}
